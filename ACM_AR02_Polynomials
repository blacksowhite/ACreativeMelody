#include <stdio.h>
#include <stdlib.h>

int main(int argc, const char * argv[]) {
    int srcPoly[3]={0};
    int temp[21]={0};
    int polynomial[21]={0};
    int power= 0;
    //int NumofTerms = 0;
    int Power2Numofterms[10]={0,3,5,7,9,11,13,15,17,19};
    int iMulti = 0,iTempTerms = 0, iSrcTerms = 0, iCopy = 0;
    //get the mount of case of the program 完成!!!
    int caseMount;

    scanf("%d",&caseMount);
    while (caseMount--) {
        
    scanf("%d %d %d %d",&srcPoly[2],&srcPoly[1],&srcPoly[0],&power);
        for (iCopy = 0; iCopy < 3; iCopy++) {
            temp[iCopy]=srcPoly[iCopy];
        }
        if (power == 1) {
        printf("%d %d %d",srcPoly[2],srcPoly[1],srcPoly[0]);
    }
    //此段完成
        else{
            for (iMulti = 1; iMulti < power; iMulti++) {
                for (iTempTerms = 0; iTempTerms < Power2Numofterms[iMulti]; iTempTerms++) {
                    for (iSrcTerms = 0; iSrcTerms < 3; iSrcTerms++) {
                        polynomial[iTempTerms+iSrcTerms] += srcPoly[iSrcTerms]*temp[iTempTerms];
                    }
                }
                //polynomial copied to temp
                for (iCopy = 0; iCopy < Power2Numofterms[iMulti] + 2; iCopy++) {
                    temp[iCopy] = polynomial[iCopy];
                    polynomial[iCopy] = 0;
                }
            }
        }
        for (int i = Power2Numofterms[iMulti]; i > 1 ;i--) {
            printf("%d ",temp[i-1]);
        }
        printf("%d",temp[0]);
        printf("\n");
        for (iCopy=0; iCopy<21; iCopy++) {
            polynomial[iCopy]=0;
        }
    }
    return 0;
}
